{
    "manifest_version": 2,
    "name": "Frontdoor",
    "version": "1.0.0",
    "description": "Chrome Extension for Frontdoor",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": [
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "https://*/*",
          "http://*/*"
        ],
        "css": ["index.css"],
        "js": ["index.js"],
        "run_at": "document_end"
      }
    ],
    "browser_action": {
      "default_popup": "index.html"
    }
  }
  