{
    "name": "frontdoor-technical-challenge",
    "version": "1.0.0",
    "description": "A Chrome extension built with React, NestJS, and TypeScript, allowing users to highlight text and receive AI-generated summaries.",
    "main": "./backend/src/main.ts",
    "browser": {
        "fs": false,
        "child_process": false,
        "net": false,
        "tls": false
    },
    "scripts": {
        "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
        "start:server": "cd backend && npm run start:dev",
        "start:client": "cd frontend && webpack serve --config ../webpack.config.js",
        "build": "npm run build:server && npm run build:client",
        "build:server": "cd backend && npm run build",
        "build:client": "cd frontend && npm run build",
        "test": "cd backend && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/chdavis0917/frontdoor-technical-challenge"
    },
    "keywords": [
        "frontdoor",
        "chrome",
        "extension",
        "react",
        "nestjs",
        "typescript"
    ],
    "author": "Christopher Davis",
    "license": "MIT",
    "dependencies": {
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.21.4",
        "@nestjs/graphql": "^11.0.4",
        "@nestjs/typeorm": "^9.0.1",
        "axios": "^1.3.5",
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mongoose": "^7.0.3",
        "openai": "^3.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.10.0",
        "react-scripts": "^5.0.1",
        "typescript": "^5.0.4",
        "webextension-polyfill": "^0.10.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "@types/react": "^18.0.33",
        "@types/react-dom": "^18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "concurrently": "^8.0.1",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.7",
        "ts-jest": "^29.1.0",
        "webpack": "^5.78.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.13.2"
    }
}
