{
  "name": "frontdoor-technical-challenge",
  "version": "1.0.0",
  "description": "google chrome extension using nestJS",
  "main": "src/main.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nest start",
    "dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chdavis0917/frontdoor-technical-challenge.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "frontdoor",
    "openai"
  ],
  "author": "Christopher Davis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chdavis0917/frontdoor-technical-challenge/issues"
  },
  "homepage": "https://github.com/chdavis0917/frontdoor-technical-challenge#readme",
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "mongodb": "^5.2.0",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.0",
    "@nestjs/schematics": "^9.4.0",
    "@nestjs/testing": "^9.4.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.16",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
