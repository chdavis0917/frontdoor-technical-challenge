{
  "name": "frontdoor-technical-challenge",
  "version": "1.0.0",
  "description": "google chrome extension using nestJS",
  "main": "./src/main.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nest start",
    "start:dev": "PORT=3001 nest start",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest ./test/**/*.test.ts",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chdavis0917/frontdoor-technical-challenge.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "frontdoor",
    "openai"
  ],
  "author": "Christopher Davis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chdavis0917/frontdoor-technical-challenge/issues"
  },
  "homepage": "https://github.com/chdavis0917/frontdoor-technical-challenge#readme",
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@platohq/nestjs-openai": "^0.0.3",
    "chatgpt": "^5.2.2",
    "dotenv": "^16.0.3",
    "mongodb": "^5.2.0",
    "nestjs-rate-limiter": "^3.1.0",
    "openai": "^3.2.1",
    "openai-api": "^1.3.1",
    "rxjs": "^7.8.0",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.13"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
