{
  "name": "frontdoor-technical-challenge",
  "version": "1.0.0",
  "description": "google chrome extension using nestJS",
  "main": "./src/main.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "nest start",
    "start:dev": "PORT=3001 nest start",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chdavis0917/frontdoor-technical-challenge.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "frontdoor",
    "openai"
  ],
  "author": "Christopher Davis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chdavis0917/frontdoor-technical-challenge/issues"
  },
  "homepage": "https://github.com/chdavis0917/frontdoor-technical-challenge#readme",
  "dependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.4.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.4",
    "openai": "^3.2.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/testing": "^9.4.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testMatch": [
      "**/test/*.test.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testTimeout": 30000
  }
}
